{
  "name": "tackle-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "extract": "i18next --config i18next-parser.config.js",
    "start": "node scripts/start.js -r @cypress/instrument-cra",
    "build": "node scripts/build.js",
    "build:dev": "GENERATE_SOURCEMAP=true node scripts/build.js",
    "build:instrumentation": "CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build",
    "test": "node scripts/test.js",
    "ui:start": "node ../server/index.js"
  },
  "dependencies": {
    "@konveyor/lib-ui": "^2.0.0",
    "@patternfly/patternfly": "4.132.2",
    "@patternfly/react-charts": "6.15.20",
    "@patternfly/react-core": "4.152.4",
    "@patternfly/react-table": "4.29.58",
    "@patternfly/react-tokens": "4.11.2",
    "@react-keycloak/web": "^3.4.0",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.2",
    "axios": "^0.21.1",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "i18next": "^19.8.4",
    "i18next-http-backend": "^1.0.22",
    "keycloak-js": "12.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.5",
    "react-measure": "^2.5.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^5.1.0",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^4.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-measure": "^2.0.6",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "@types/yup": "^0.29.11",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.19.1",
    "camelcase": "^6.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cypress": "8.4.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-keycloak-commands": "^1.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.7.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.7",
    "i18next-parser": "^3.6.0",
    "jest": "^27.4.7",
    "jest-enzyme": "^7.1.2",
    "jest-resolve": "^27.4.6",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^2.5.2",
    "prettier": "^2.2.1",
    "react-dev-utils": "^12.0.0",
    "react-refresh": "^0.11.0",
    "redux-devtools-extension": "^2.13.8",
    "resolve": "^1.22.0",
    "resolve-url-loader": "^4.0.0",
    "semver": "^7.3.5",
    "source-map-explorer": "^2.5.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "typescript": "4.0.3",
    "webpack": "^5.67.0",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^4.1.1",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "pkg/client/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/home/gildub/github.com/gildub/tackle-ui/pkg/client/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "npm run extract"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "nyc": {
    "exclude": "pkg/client/src/reportWebVitals.ts"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
